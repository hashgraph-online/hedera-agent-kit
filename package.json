{
  "name": "hedera-agent-kit",
  "version": "1.1.0",
  "type": "module",
  "main": "./dist/node/index.cjs",
  "module": "./dist/node/index.mjs",
  "browser": {
    "./dist/browser/hedera-agent-kit.umd.js": "./dist/browser/hedera-agent-kit.umd.js",
    "./dist/browser/hedera-agent-kit.es.js": "./dist/browser/hedera-agent-kit.es.js"
  },
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/node/index.mjs",
      "require": "./dist/node/index.cjs",
      "browser": [
        "./dist/browser/hedera-agent-kit.es.js",
        "./dist/browser/hedera-agent-kit.umd.js"
      ],
      "types": "./dist/types/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "demo:langchain": "tsx tests/index.ts",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:integration": "NODE_OPTIONS=--experimental-vm-modules jest tests/integration/hts.integration.test.ts",
    "prepare": "npm run build",
    "build": "npm run clean && npm run build:node && npm run build:browser",
    "build:node": "vite build --mode node",
    "build:browser": "vite build --mode browser",
    "clean": "rm -rf dist",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-sonarjs": "^3.0.2",
    "tsx": "^4.19.4",
    "typescript": "^5.7.2",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^3.9.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@swc/core": "^1.7.0",
    "@swc/jest": "^0.2.36"
  },
  "dependencies": {
    "@hashgraph/sdk": "^2.63.0",
    "@hashgraphonline/hashinal-wc": "^1.0.96",
    "@hashgraphonline/standards-agent-kit": "0.0.22",
    "@hashgraphonline/standards-sdk": "^0.0.97",
    "@langchain/core": "^0.3.55",
    "@langchain/langgraph": "^0.2.71",
    "@langchain/openai": "^0.5.10",
    "bignumber.js": "^9.3.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "zod": "^3.24.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hedera-dev/hedera-agent-kit"
  }
}
